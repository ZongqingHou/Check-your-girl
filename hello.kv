<ShowImageLayout@BoxLayout>:
    source: None
    Image:
        source: root.source

<GoodBtn@Button>:
    text: 'Good :)'
    on_release: root.onclick()

<NotGoodBtn@Button>:
    text: 'Not good :('
    on_release: root.onclick()

<SexyBtn@Button>:
    text: 'Sexy'
    on_release: root.onclick()

<NotClearBtn@Button>:
    text: 'Not clear'
    on_release: root.onclick()